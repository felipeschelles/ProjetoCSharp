<Window x:Class="TelaInicial.EditarConta"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="EditarConta" Height="480" Width="825">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="C:\Users\Felipe\Source\Repos\ProjetoCSharp\BancoEletronico\TelaInicial\TestBack1.png"/>
        </Grid.Background>
        <Label x:Name="lblEditarConta" Content="Editar Conta" HorizontalAlignment="Left" Margin="44,36,0,0" VerticalAlignment="Top" IsEnabled="False" FontSize="28"/>
        <Label x:Name="lblEditarConta1" Content="Tipo de conta:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="44,157,0,0"/>
        <ComboBox x:Name="comboBoxEditarTipoConta" HorizontalAlignment="Left" Margin="161,160,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="comboBoxEditarTipoConta_SelectionChanged"/>
        <Button x:Name="btnVoltarEditarConta" Content="Voltar" HorizontalAlignment="Left" Margin="733,420,0,0" VerticalAlignment="Top" Width="75" Click="btnVoltarEditarConta_Click"/>
        <Label x:Name="llId" Content="id do cliente" HorizontalAlignment="Left" Margin="44,113,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtIdConta" HorizontalAlignment="Left" Height="23" Margin="161,113,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnVerificarEditar" Content="Pesquisar" HorizontalAlignment="Left" Margin="326,113,0,0" VerticalAlignment="Top" Width="75" Click="btnVerificarEditar_Click"/>
        <Button x:Name="btnEditarConta" Content="Editar" HorizontalAlignment="Left" Margin="612,420,0,0" VerticalAlignment="Top" Width="75" Click="btnEditarConta_Click"/>

    </Grid>
</Window>
