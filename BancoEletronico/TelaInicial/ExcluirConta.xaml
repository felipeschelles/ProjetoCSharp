        <ComboBox x:Name="cboxConta"  HorizontalAlignment="Left" Margin="131,72,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cboxConta_SelectionChanged">
            <ComboBoxItem x:Name="contaC" Content="Conta Corrente" HorizontalAlignment="Left" Width="118"/>
            <ComboBoxItem x:Name="contaP" Content="Conta Poupanca" HorizontalAlignment="Left" Width="118"/>
        </ComboBox>
                <Label Content="Tipo de Conta:" HorizontalAlignment="Left" Margin="39,68,0,0" VerticalAlignment="Top"/>

    </Grid>
